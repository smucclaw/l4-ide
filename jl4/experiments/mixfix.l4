DECLARE Person
  HAS name IS A STRING

GIVEN person1 IS A Person
      person2 IS A Person
      child   IS A Person
GIVETH A Person
person1 `copulated with` person2 `to make` child MEANS
  child

alice   MEANS Person WITH name IS "Alice Apple"
bob     MEANS Person WITH name IS "Roberto Boberto"
charlie MEANS Person WITH name IS "Charles Camelotte"

#EVAL (alice) `copulated with` (bob) `to make` charlie

#EVAL  alice  `copulated with`  bob  `to make` charlie


-- #EVAL                     alice
