DECLARE Colour IS ONE OF red, green, blue

DECLARE Folder1
HAS name IS A STRING, colour IS A Colour

DECLARE Folder2
HAS name IS A STRING
    colour IS A Colour

DECLARE Folder3
HAS name: A STRING
    colour: A Colour

