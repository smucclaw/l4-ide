DECLARE Person
    HAS
        name IS A STRING
ASSUME walks  IS A BOOLEAN
ASSUME drinks IS A BOOLEAN
ASSUME eats   IS A BOOLEAN

GIVEN p [person] IS A Person
DECIDE `must sing`
    IF        walks [the person must walk]
       AND    eats [the person must eat]
           OR drinks

