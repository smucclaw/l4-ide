// Test file for ENV functionality

DECIDE testHomeExists IS
  ENV "HOME"

DECIDE testUserExists IS
  ENV "USER"

DECIDE testNonExistent IS
  ENV "THIS_VAR_SHOULD_NOT_EXIST_12345"

DECIDE testHomeValue IS
  CONSIDER testHomeExists
    WHEN JUST val THEN val
    WHEN NOTHING THEN "HOME not found"

#EVAL testHomeExists
#EVAL testUserExists
#EVAL testNonExistent
#EVAL testHomeValue
